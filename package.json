{
  "name": "steamlauncher",
  "productName": "SteamLauncher",
  "version": "0.0.2",
  "description": "SteamLauncher lorem ipsum",
  "keywords": [],
  "homepage": "https://github.com/Sak32009/SteamLauncher/",
  "bugs": {
    "url": "https://github.com/Sak32009/SteamLauncher/issues/"
  },
  "license": "MIT",
  "author": {
    "name": "Sak32009",
    "url": "https://sak32009.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sak32009/SteamLauncher.git"
  },
  "private": true,
  "main": "./src/main/dist/index.js",
  "scripts": {
    "dev": "yarn concurrently \"yarn:dev:*\"",
    "dev:main": "yarn vite build --mode development -w -c ./src/main/vite.config.ts",
    "dev:preload": "yarn vite build --mode development -w -c ./src/preload/vite.config.ts",
    "dev:render": "yarn vite build --mode development -w -c ./src/render/vite.config.ts",
    "dev:electron": "yarn electron .",
    "prod": "yarn run-s prod:*",
    "prod:main": "yarn vite build --mode production -c ./src/main/vite.config.ts",
    "prod:preload": "yarn vite build --mode production -c ./src/preload/vite.config.ts",
    "prod:render": "yarn vite build --mode production -c ./src/render/vite.config.ts",
    "prod:electron": "yarn electron .",
    "package": "yarn run-s package:*",
    "package:lint": "yarn run lint",
    "package:bundle": "yarn run bundle",
    "package:build": "yarn fse remove ./build/release && yarn electron-builder build -p never --x64 --dir",
    "dist": "yarn run-s dist:*",
    "dist:lint": "yarn run lint",
    "dist:bundle": "yarn run bundle",
    "dist:build": "yarn fse remove ./build/release && yarn electron-builder build -p never --x64",
    "release": "yarn run-s release:*",
    "release:lint": "yarn run lint",
    "release:bundle": "yarn run bundle",
    "release:build": "yarn fse remove ./build/release && yarn electron-builder build -p always --x64",
    "bundle": "yarn run-s bundle:*",
    "bundle:main": "yarn vite build -c ./src/main/vite.config.ts",
    "bundle:preload": "yarn vite build -c ./src/preload/vite.config.ts",
    "bundle:render": "yarn vite build -c ./src/render/vite.config.ts",
    "lint": "tsc --noEmit && xo .",
    "jquery": "yarn fse copy \"./node_modules/jquery/dist/jquery.min.js\" \"./src/render/src/public/assets/js/jquery.min.js\"",
    "postinstall": "yarn electron-builder install-app-deps && yarn run jquery"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@fontsource/oswald": "^4.5.1",
    "@iconify/iconify": "^2.1.0",
    "@iconify/json": "^1.1.450",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/ini": "^1.3.31",
    "@types/jquery": "^3.5.11",
    "@types/markdown-it": "^12.2.3",
    "@types/mustache": "^4.1.2",
    "@types/steamid": "^2.0.1",
    "@types/tailwindcss": "^3.0.0",
    "@types/toastr": "^2.1.39",
    "autoprefixer": "^10.4.1",
    "concurrently": "^6.5.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "electron-store": "^8.0.1",
    "fs-extra": "^10.0.0",
    "github-markdown-css": "^5.1.0",
    "ini": "^2.0.0",
    "jquery": "^3.6.0",
    "markdown-it": "^12.3.0",
    "mustache": "^4.2.0",
    "navigo": "^8.11.1",
    "node-fetch": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "steamid": "^2.0.0",
    "tailwindcss": "^3.0.8",
    "toastr": "^2.1.4",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-externals": "^0.3.2",
    "vite-plugin-purge-icons": "^0.7.0",
    "xo": "^0.47.0"
  },
  "dependencies": {}
}
