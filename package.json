{
  "name": "steamlauncher",
  "productName": "SteamLauncher",
  "version": "0.0.3",
  "description": "SteamLauncher optimizes the setup process of each game in a simple and automatic way.",
  "keywords": [],
  "homepage": "https://github.com/Sak32009/SteamLauncher/",
  "bugs": {
    "url": "https://github.com/Sak32009/SteamLauncher/issues/"
  },
  "license": "MIT",
  "author": {
    "name": "Sak32009",
    "url": "https://sak32009.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sak32009/SteamLauncher.git"
  },
  "private": true,
  "main": "./src/main/dist/index.js",
  "scripts": {
    "start-app": "yarn electron .",
    "dev": "yarn concurrently 'yarn:dev:*'",
    "prod": "yarn run-s 'prod:*'",
    "package": "yarn run-s 'package:*'",
    "installer": "yarn run-s 'installer:*'",
    "release": "yarn run-s 'release:*'",
    "bundle": "yarn run-s 'bundle:*'",
    "dev:main": "yarn vite build --mode development -w -c ./src/main/vite.config.ts",
    "dev:preload": "yarn vite build --mode development -w -c ./src/preload/vite.config.ts",
    "dev:render": "yarn vite --mode development -c ./src/render/vite.config.ts",
    "prod:main": "yarn vite build --mode production -c ./src/main/vite.config.ts",
    "prod:preload": "yarn vite build --mode production -c ./src/preload/vite.config.ts",
    "prod:render": "yarn vite build --mode production -c ./src/render/vite.config.ts",
    "package:lint": "yarn run lint",
    "package:bundle": "yarn run bundle",
    "package:build": "yarn electron-builder build -p never --x64 --dir",
    "installer:lint": "yarn run lint",
    "installer:bundle": "yarn run bundle",
    "installer:build": "yarn electron-builder build -p never --x64",
    "release:lint": "yarn run lint",
    "release:bundle": "yarn run bundle",
    "release:build": "yarn electron-builder build -p always --x64",
    "bundle:main": "yarn vite build -c ./src/main/vite.config.ts",
    "bundle:preload": "yarn vite build -c ./src/preload/vite.config.ts",
    "bundle:render": "yarn vite build -c ./src/render/vite.config.ts",
    "lint": "tsc --noEmit && xo .",
    "jquery": "yarn fse copy \"./node_modules/jquery/dist/jquery.min.js\" \"./src/render/src/public/js/jquery.min.js\"",
    "postinstall": "yarn electron-builder install-app-deps && yarn run jquery"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@fontsource/oswald": "^4.5.4",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@mdi/font": "^6.5.95",
    "@popperjs/core": "^2.11.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/bootstrap": "^5.1.9",
    "@types/ini": "^1.3.31",
    "@types/jquery": "^3.5.14",
    "@types/markdown-it": "^12.2.3",
    "@types/mustache": "^4.1.2",
    "@types/node": "^17.0.21",
    "@types/steamid": "^2.0.1",
    "@types/tailwindcss": "^3.0.9",
    "autoprefixer": "^10.4.2",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.0.0",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "github-markdown-css": "^5.1.0",
    "jquery": "^3.6.0",
    "mustache": "^4.2.0",
    "navigo": "^8.11.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.2",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "xo": "^0.48.0"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "ini": "^2.0.0",
    "markdown-it": "^12.3.2",
    "nanoid": "^3.3.1",
    "steamid": "^2.0.0"
  }
}
